{
  "writeKey": "a7452714318159361472-1fd1632b9eff9ca6b595a1011bf13f002b3371602a5c0d95d3d3894868405a12",
  "readKey": "a7452714318159361472-04fa791b8f175f1cef88c8ff2cf43c5344a6894149e11daaf996b60e95d74cb4",

  "upload": {
    "type": "neon",
    "files": [
      {
        "pattern": "app/lang/locale.en.neon",
        "lang": "inherited",
        "path": "${path}"
      },
      {
        "pattern": "app/lang/locale+intl-icu.en.neon",
        "lang": "inherited",
        "path": "${path}"
      }
    ],
    "features": [
      "plural_icu"
    ]
  },

  "download": {
    "files": "${path}/${fileNameWithoutExtension}.${lang}.neon"
  },

  "transformations": [
    {
      "name": "fileNameWithoutExtension",
      "source": "${file}",
      "operations": [
          "substringBefore: ."
      ]
    }
  ]
}
